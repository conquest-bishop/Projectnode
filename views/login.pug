      span.navbar-toggler-icon
   
.container-cards
.row
  .col-sm-6
    h1.card-title.card-title-parking Log in
  .col-sm-6
    .card.card1.gy-5
      .card-body
        form#formId(name='register'  method='POST', action="/api/login")
            label.form-label(for='exampleInputEmail1') Email
            input#exampleInputEmail1.form-control(type='email' name='emailinput' aria-describedby='emailHelp')
            span#emailError
            label.form-label(for='Password') Password
            input.form-control(type='password' name='password' aria-label='Phone Number')
            span#phoneError
            br
            .butt
                  button.btn(type='submit') login
